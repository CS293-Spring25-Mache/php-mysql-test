{
    "name": "PHP & MySQL Dev Container",
    "dockerComposeFile": "docker-compose.yml",
    "service": "php",
    "workspaceFolder": "/workspace",
    "extensions": [
      "bmewburn.vscode-intelephense-client",
      "ms-mysql.mysql"
    ],
    "forwardPorts": [3306, 8080],
    "postCreateCommand": "composer install",
    "remoteUser": "vscode"
  }
  